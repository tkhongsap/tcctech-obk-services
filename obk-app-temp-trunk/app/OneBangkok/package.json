{
  "name": "OneBangkok",
  "version": "1.7.0",
  "private": true,
  "scripts": {
    "setLocal": "ENVFILE=.env",
    "setSit": "ENVFILE=.env.sit",
    "setDev": "ENVFILE=.env.dev",
    "setUat": "ENVFILE=.env.uat",
    "setProd": "ENVFILE=.env.prod",
    "node:clean": "rm -rf node_modules&& yarn cache clean && yarn install",
    "android": "react-native run-android -- --reset-cache",
    "ios": "react-native run-ios -- --reset-cache",
    "ios:dev": "yarn setDev  react-native run-ios --mode=Debug --scheme \"OneBangkok\"",
    "ios:sit": "yarn setSit react-native run-ios --mode=Debug --scheme \"OneBangkokSIT\"",
    "ios:uat": "yarn setUat react-native run-ios --mode=Debug --scheme \"OneBangkokUAT\"",
    "ios:prod": "yarn setProd react-native run-ios --mode=Debug --scheme \"OneBangkokPROD\"",
    "android:dev": "yarn setDev  react-native run-android --mode=devDebug --appIdSuffix=dev",
    "android:sit": "yarn setSit  react-native run-android --mode=sitDebug --appIdSuffix=sit",
    "android:uat": "yarn setUat  react-native run-android --mode=uatDebug --appIdSuffix=uat",
    "android:prod": "yarn setProd  react-native run-android --mode=prodDebug --appIdSuffix=prod",
    "ios-14": "react-native run-ios --simulator=\"iPhone 14 Pro\" -- --reset-cache",
    "ios-vincent": "react-native run-ios --device \"Vincent van kok\" -- --verbose",
    "lint": "eslint .",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "android:bundle:assets": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "android:remove:assets": "rm -rf android/app/src/main/res/drawable-hdpi &&  rm -rf android/app/src/main/res/drawable-mdpi &&  rm -rf android/app/src/main/res/drawable-xhdpi && rm -rf android/app/src/main/res/drawable-xxhdpi && rm -rf android/app/src/main/res/drawable-xxxhdpi && rm -rf android/app/src/main/res/raw",
    "android:clean": "yarn node:clean && cd android && ./gradlew clean",
    "open-apk": "open ./android/app/build/outputs/apk/",
    "android:dev-release": "yarn setDev && cd android && ./gradlew assembleDevRelease",
    "android:sit-release": "yarn setSit && cd android && ./gradlew assembleSitRelease",
    "android:uat-release": "yarn setUat && cd android && ./gradlew bundleUatRelease",
    "android:prod-release": "yarn setProd && cd android && ./gradlew bundleProdRelease",
    "bump-build": "npx react-native bump-version --skip-semver-for all",
    "bump-build:ios": "npx react-native bump-version --skip-semver-for all --skip-code-for android",
    "bump-build:android": "npx react-native bump-version --skip-semver-for all --skip-code-for ios",
    "bump-patch": "npx react-native bump-version --type patch",
    "bump-minor": "npx react-native bump-version --type minor"
  },
