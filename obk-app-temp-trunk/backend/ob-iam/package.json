{
  "name": "ob-iam",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node dist/app.js",
    "build": "npx openapi-typescript src/openapi/openapi_spec.yaml -o src/openapi/interfaces/index.ts && ts-node src/app.ts",
    "dev": "npx prisma generate --schema=db/schema.prisma && npx prisma migrate deploy --schema=db/schema.prisma && nodemon --watch 'src/**/*.*' --exec 'yarn run build' --ignore 'src/openapi/interfaces/index.ts'",
    "docker-build": "docker build -t ob-iam .",
    "docker-start": "yarn tsc && docker build -t ob-iam . && docker run -p 3000:3000 -v $(pwd):/app -v /app/node_modules ob-iam nodemon -L --watch 'dist' --exec 'node dist/app.js'",
    "prisma:generate": "npx prisma generate --schema=db/schema.prisma",
    "prisma:migrate:generate": "yarn run prisma:migrate:generate-internal --name",
    "prisma:migrate:generate-internal": "npx prisma migrate dev --schema=db/schema.prisma",
    "prisma:migrate:deploy": "npx prisma migrate deploy --schema=db/schema.prisma",
    "prisma:migrate:format": "npx prisma format --schema=db/schema.prisma",
    "lint": "eslint . --ext .ts --max-warnings=0",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "test": "jest --forceExit --detectOpenHandles --passWithNoTests",
    "test:coverage": "jest --coverage && open coverage/lcov-report/index.html",
    "tsc": "npx tsc --noEmit --pretty",
    "generate:openapi": "npx openapi-typescript src/openapi/openapi_spec.yaml -o src/openapi/interfaces/index.ts",
    "obsdk:sync:interface": "cp src/openapi/interfaces/index.ts ../../app/OneBangkok/src/utils/ob_sdk/services/ob_iam/openapi_interfaces.ts"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.363.0",
    "@aws-sdk/client-sns": "^3.363.0",
    "@prisma/client": "4.15.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.3",
    "@types/otp-generator": "^4.0.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "express": "^4.18.2",
    "express-http-context": "^1.2.4",
    "express-openapi-validator": "^5.0.4",
    "fs": "^0.0.1-security",
    "http-errors": "^2.0.0",
    "jest": "^29.5.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "ob-bms-sdk": "^0.0.20",
    "openapi-typescript": "https://gitpkg.now.sh/stt-mtel/openapi-typescript/packages/openapi-typescript?main",
    "otp-generator": "^4.0.1",
    "prettier": "^2.8.8",
    "prisma": "4.15.0",
    "supertest": "^6.3.3",
    "swagger-routes-express": "^3.3.2",
    "ts-jest": "^29.1.0",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "winston": "^3.8.2",
    "winston-context": "^0.0.7",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/lodash": "^4.14.195",
    "@types/luxon": "^3.3.0",
    "@types/yamljs": "^0.2.31",
    "husky": "^8.0.3",
    "nodemon": "^2.0.22",
    "prisma": "5.5.2"
  },
  "peerDependencies": {
    "ob-common-lib": "*"
  }
}
