{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "es2020"],
    "jsx": "react-native",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true, // <-- Add this line
    "skipLibCheck": true,
    "strict": true,
    "noImplicitAny": false
  }
}
